<div class ="container">
<%=form_for @user, url: sessions_path, method: 'post' do |f|%>
    <div class = 'row'>
    <%=f.label :username, "Username"%>
    <%=f.text_field :username%>
    <br>
    <%=f.label :password, "Password"%>
    <%=f.password_field :password%>
    <br>
    </div>
    <div class = 'row'>
        <%=f.submit 'Signin'%>
    </div>
<%end%>
</div>