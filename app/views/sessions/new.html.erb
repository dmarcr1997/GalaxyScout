<div class ="container">
<%=form_for @user, url: sessions_path, method: 'post' do |f|%>
    <div class = 'row'>
    <%=f.label :email, "Email"%>
    <%=f.text_field :email%>
    <br>
    <%=f.label :password, "Password"%>
    <%=f.password_field :password%>
    <br>
    </div>
    <div class = 'row'>
        <%=f.submit 'Signin'%>
<%end%>
<div class = 'facebook_login_button'>
<%= link_to('Log in with Facebook!', '/auth/facebook') %>
</div>
</div>
</div>